{
  "name": "aws-test",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "refmterr dune build -j 4",
    "release": { "releasedBinaries": [ "AwsTestApp.exe" ] }
  },
  "buildDirs": {
    "test": {
      "require": [ "aws-test.lib" ],
      "main": "TestAwsTest",
      "name": "TestAwsTest.exe"
    },
    "library": { 
      "name": "aws-test.lib", 
      "require": ["aws-s3", "aws-s3-lwt"],
      "namespace": "AwsTest" 
    },
    "executable": {
      "require": [ "aws-test.lib", "pcre" ],
      "main": "AwsTestApp",
      "name": "AwsTestApp.exe"
    }
  },
  "scripts": { 
    "test": "esy x TestAwsTest.exe",
    "format": "refmt --in-place library/*.re" },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/aws-s3": "4.1.0",
    "@opam/aws-s3-lwt": "4.1.0",
    "@opam/dune": "1.4.0",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": { "@esy-ocaml/merlin": "*" },
  "resolutions": {
    "@opam/conf-libpcre": "github:joprice/pcre-ocaml#f563fdffa84b0dbe2c3f03998d897b5854e26805"
    /*"@opam/conf-libpcre": "link:/Users/josephprice/dev/github.com/joprice/pcre-ocaml"*/
  }
}
